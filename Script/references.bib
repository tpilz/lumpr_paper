@ARTICLE{Vivoni2004,
  author = {Enrique R. Vivoni and Valeri Y. Ivanov and Rafael L. Bras and Dara
	Entekhabi},
  title = {Generation of Triangulated Irregular Networks Based on Hydrological
	Similarity},
  journal = {J. Hydrol. Eng.},
  year = {2004},
  volume = {9},
  pages = {288--302},
  number = {4},
  abstract = {Aim:
	
	- use of multiple-resolution triangulated irrgular network (TIN) mesh
	to represent the surface terrain in a watershed model
	
	- presentation of a GIS methodology for constructing TIN terrain models
	
	
	Features:
	
	- TIN models are capable of capturing topographic variability that
	cannot be achieved via raster aggregation
	
	- TINs are a promising tool for assessing DEM quality
	
	- flat or gently sloping catchment can be better represented by TINs
	(fewer nodes and improved aqccuracy for a given tolerance level)
	
	- TINs simulate larger domains better than raster-based models
	
	- TINs generated with focus on hydrological similarity better at capturing
	hydrological variability than hydrographic TINs
	
	- TINs permit smooth transition between continental basin and multiple
	nested watersheds; hydrologic signature at each scale can be preserved
	
	
	Limitations:
	
	- aggregation errors propagating to predictions of a hydrological
	model may occur based on the selected method and the chosen acceptable
	error level
	
	- resolution errors might occur (e.g. if floodplain area is not represented
	acurately as dynamics of variable saource area is then not well captured)
	
	- relative complexity of data structures and algorithms in comparison
	to raster grid-based approaches
	
	
	Motivation:
	
	- traditional representation of terrain data in hydrological models:
	
	 1. aggregation or resampling DEMs to coearser resolution
	
	 2. topographic distribution functions
	
	- no approach can incorporate all igh-resolution topographic data
	available
	
	- need for efficient method representing high-resolution terrain with
	minimal loss of information
	
	
	Triangulated Irregular Networks (TIN):
	
	- computational structures to represent topography: regular grids,
	contour lines, TINs
	
	- TIN data structure: piece-wise linear interpolation of a set of
	points of a DEM resulting in nonoverlapping triangular elements varying
	in size
	
	- Delaunay triangulation as preferred method
	
	- motivation for representation of watershed topography:
	
	 1. variable resolution by irregular domain
	
	 - computational savings as number of nodes can be reduced in regions
	of low terrain variability
	
	 - allows TIN-based models to operate over large regions
	
	 2. linear features can be preserved in the model mesh
	
	 - e.g. terrain breaklines, stream networks, boundaries between heterogeneous
	regions
	
	 - no artifacts as in raster discretization
	
	- only few studies addressed TIN methods
	
	
	Topographic (traditional) TIN:
	
	- selective sampling of high-resolution DEM according to a topographic
	criterion
	
	- preserves key topographic characteristics
	
	- GIS sampling rechniques:
	
	 - very important point (VIP) method, local procedure, determining
	the significance of a point relative to a filter
	
	 - drop heuristic (DH) method, global procedure, successively reduces
	DEM points and retains only significant points
	
	
	Hydrographic TIN:
	
	- combines traditional approach with representation of surface streams,
	basin boundaries, and floodplains
	
	1. stream network calculation from a DEM
	
	 - various algorithms exist
	
	 - herein a constant area method is used to classify DEM points as
	stream cells
	
	 - simplification and generalization to remove raster effects
	
	2. boundary delineation from a DEM
	
	 - create depressionless DEM
	
	 - derive overland flow directions
	
	 - compute upslope area at each outlet
	
	 - simplification and generalization to remove raster effects
	
	3. flood plains and riparian zones
	
	 - integrate existing floodplain model from field work OR
	
	 - algorithm for delineation from DEM
	
	 - here, elevation-threshold algorithm is used
	
	4. landscape features
	
	 - available landscape feature maps (soil, land use, geology, etc.)
	can be directly included OR
	
	 - combined features such as HRUs can be included
	
	
	Hydrological similarity TIN:
	
	- new method to cature hydrologic dynamics in TINs using a measure
	of hydrological similarity
	
	- here, topographic index (TOPMODEL) is used to derive topographic
	distribution classes
	
	 - determine steady-state hydrologic response in a basin
	
	 - measure of hydrological similarity for catchments dominated by
	saturation excess runoff
	
	 - divides areas saturating frequently from upslope regions that lack
	runoff production
	
	 - in frequently saturated areas (high index value) more points in
	the TIN are retained
	
	- establish a functional relationship between index class and distance
	between selected DEM points
	
	- hydrological similarity TIN then samples a raster DEM with a variable
	resolution filter
	
	- after point selection triangulation is used to create a TIN},
  doi = {10.1061/(ASCE)1084-0699(2004)9:4(288)},
  eprint = { http://dx.doi.org/10.1061/(ASCE)1084-0699(2004)9:4(288) },
  file = {:Vivoni_etal_2004_triangulated_irregular_networks.pdf:PDF}
}

