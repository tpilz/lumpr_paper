@ARTICLE{Cochrane2003,
  author = {Cochrane, T.A. and Flanagan, D.C.},
  title = {Representative hillslope methods for applying the WEPP model with
	DEMs and GIS},
  journal = {T. ASAE},
  year = {2003},
  volume = {46},
  pages = {1041--1049},
  number = {4},
  abstract = {Aim:
	
	- evaluation of three methods to automatically create representative
	slope profiles and two metods to create representative slope lengths
	as pre-processing for the water erosion model WEPP
	
	
	Motivation:
	
	- Water erosion prediction project (WEPP) model: process-based, continuous,
	erosion model
	
	- applicable to hillslopes and watersheds
	
	- runoff and soil loss for every rainfall event
	
	- hillslopes, channels, and relationships within the watershed have
	to be identified
	
	- hillslopes represented by representative length, width, and slope
	
	- pre-processing: manual identification of channels, hillslopes, and
	slope profiles from topographic maps or data records
	
	 - time consuming and subjective
	
	- GIS facilitate model pre-processing
	
	- so far also with GIS methods some degree of ambiguity by manually
	selecting representative hillslopes
	
	
	Methods:
	
	- WEPP model
	
	- preparing analyses of DEM with TOPAZ
	
	- ASCII output used to define watershed components and representative
	slope profiles for each hillslope
	
	- channel identification:
	
	 - delineation of channel network using a DEM
	
	 - define critical source area (CSA): drainage area whose concentrated
	water flow defines the beginning of a channel
	
	 - channel parameters have to be defined manually be the user (width,
	shape, depth, and erodibility)
	
	- hillslope identification:
	
	 - hillslope: set of grid cells draining to the left, right, or top
	of each individual channel
	
	 - extracted from DEM by identifying flowpaths with the aid of TOPAZ
	
	- representative hillslope profiles:
	
	 - can be derived from flowpaths extracted from DEM:
	
	 - flowpaths: the route water travels when flowing from one cell to
	the next, starting from a cell with no inflow and ending at a channel
	
	 - within a hillslope there can be many flowpaths which may eventually
	intersect when approaching a channel
	
	 - determined from four TOPAZ output files: file defining beginning
	and end of flowpaths, raster drainage direction file, hillslope definition
	file, cell slope file
	
	 - representative slope profile:
	
	 - should represent all individual flowpaths within a hillslope
	
	 - three algorithms: 
	
	 1. linear average: assumes that all slope gradients have equal weight
	in their influence on soil loss
	
	 2. exponentially transformed average: assumes exponential relationship
	between slope and soil loss
	
	 3. weighted average: weighting flowpaths by area and length, i.e.
	greater area and lengths assumed to contribute more
	
	- representative profile length:
	
	 - determine the length of the hillsloes in the watershed
	
	 1. Chanleng method: for laterally draining hillslopes the width set
	equal to adjacent channel length; hillslope length is then derived
	from dividing hillslope area by width assuming rectangular shape
	
	 2. Calcleng method: for top hillslopes not matching channel length;
	assumes flowpaths are routes traveled by water and larger and longer
	flowpaths contribute more
	
	
	Results:
	
	- three slope profile methods: 
	
	 - in test case only minimal differences between profiles
	
	 - no significant differences in soil loss, deposition, or sediment
	yield
	
	 - weighted average method is recommended as it is theoretically more
	robust
	
	- two slope length methods:
	
	 - no significant differences in runoff and sediment yield
	
	 - runoff seems to be more different than sediment yield
	
	 - Chanleng method recommended as being more representative, more
	consistent and easy to use},
  file = {:Cochrane_Flanagan_2003_representative_hillslope_computation.pdf:PDF},
  owner = {tobias},
  timestamp = {2016.04.28}
}

@ARTICLE{Costa-Cabral1994,
  author = {Costa-Cabral, Mariza C. and Burges, Stephen J.},
  title = {Digital Elevation Model Networks (DEMON): A model of flow over hillslopes
	for computation of contributing and dispersal areas},
  journal = {Water Resour. Res.},
  year = {1994},
  volume = {30},
  pages = {1681--1692},
  number = {6},
  abstract = {Aim:
	
	- digital elevation model networks (DEMON) to respect non-point source
	flow generation
	
	- combines advantages of contour-based models and rectangular DEMs
	
	
	Background and motivation:
	
	- specific contributing area (SCA)
	
	 - total contributing area (TCA) divided by contour segment length
	
	 - TCA: plan area of terrain contributing to a contour segment
	
	 - may be interpreted as an equivalent flow path length
	
	 - concept assumptions:
	
	 a) plan view of flow directions do not change with deth below land
	surface
	
	 b) determined by surface topography
	
	 - limitations: 
	
	 a) terrain with small permeability relative to rainfall rate
	
	 b) subsurface flow lines approximately parallel in plan view to surface
	flow lines
	
	 - application:
	
	 - indicator of discharge assuming spatially uniform and steady rate
	of flow generation (strong approximation of natural conditions)
	
	 - indices as combination of SCA and other variables
	
	 - prediction of location and extent of channel networks
	
	- specific dispersal area (SDA)
	
	 - total dispersal area (TDA) divided by contour segment length
	
	 - TDA: plan area of terrain draining flow from specific contour segment
	
	 - indicates the area of influence of flow generated at specific location
	
	 - terminates at location of convergence or at receiving water body
	
	 - concept assumptions: the same as fow SCA
	
	 - application: e.g. to predict the influence zone of upslope pollution
	sources
	
	- with availability of grid-based DEMs the concept was transferred
	from contour-based elevation maps
	
	- DEMs unsuitable for SDA prediction because of one-dimensional downslope
	flow projection
	
	
	Algorithms for SCA computation:
	
	- D8
	
	 - each pixels spills into one of its eight neighbours in direction
	of steepest descent
	
	 - errors:
	
	 a) parallel flow: 
	
	 - correct only when in cardinal direction (0, 90, 180, 270 degrees)
	
	 - underestimated by factor of 2 in diagonal direction (45, 135, 225,
	315 degrees)
	
	 - underestimated by factor between 1 and 2 in other cases
	
	 b) divergent flow
	
	 - radial flow symmetry not conserved (e.g. on a cone mountain)
	
	 - overestimation by factor of 2 or underestimation approaching up
	to 100 percent
	
	 c) convergent flow
	
	 - radial symmetrie not conserved
	
	 - over- and underestimated not as large as for a cone mountain
	
	- Rho8
	
	 -},
  doi = {10.1029/93WR03512},
  file = {:Costa-Cabral_Burges_1994_DEMON_model_contributing_dispersal_areas.pdf:PDF},
  issn = {1944-7973},
  keywords = {Monitoring networks, Streamflow, Geomorphology: general, Erosion}
}

