@ARTICLE{Reggiani1998,
  author = {Paolo Reggiani and Murugesu Sivapalan and S. Majid Hassanizadeh},
  title = {A unifying framework for watershed thermodynamics: balance equations
	for mass, momentum, energy and entropy, and the second law of thermodynamics},
  journal = {Advances in Water Resources },
  year = {1998},
  volume = {22},
  pages = {367--398},
  number = {4},
  abstract = {Aim:
	
	- develop a general framework for hydrological model development respecting
	balances of mass, energy, and momentum using advantages of distributed
	and lumped models
	
	
	Objectives:
	
	- employing an averaging approach from flow porous media to derive
	watershed scale balance equations for mass, momentum, energy, and
	entropy
	
	- watershed divided into smaller entities over which equations are
	averaged in space and time
	
	- definition of jump conditions for transfer of mass, momentum, energy
	and entropy across system boundaries
	
	- constitutive relationships for mass exchange between stores and
	surroundings in compliance with second law of thermodynamics
	
	
	Motivation:
	
	- physically based distributed models:
	
	 - advantage: explicitly consider conservation of mass and momentum
	
	 - shortcommings: computationally overwhelming, need for detailed
	information mostly not available, detailed output often not needed
	for practical application, over parameterization and poor parameter
	identifiability
	
	- lumped conceptual models:
	
	 - advantage: do not need detailed information as watershed is represented
	as connected conceptual storages
	
	 - disadvantages: only respect the mass balance, parameters often
	lack a physical meaning
	
	- no accepted general framework for describing a hydrological system
	
	- paper motivated by approach for multi-phase flow in porous media
	by Hassanizadeh and Gray
	
	
	Framework:
	
	- watershed as an open thermodynamic system (mass, momentum, energy
	and entropy are exchanged with surrounding regions)
	
	- exchange driven by mass input, extraction, and transfer
	
	- fundamental components of a watershed: hillslopes organized around
	a river network
	
	- division of the watershed into smaller elements defined as representative
	elementary watersheds (REW)
	
	- averaging of conservation equations over the REW
	
	- incorporation of sub-REW variability in an effective manner
	
	- time interval chosen application-specific whereas smaller temporal
	fluctuations are filtered out
	
	- determination of relationships between exchange terms and catchment
	storages in conservation equations
	
	
	REW:
	
	- autonomous functional watershed unit
	
	- includes all basic functional components: channels and hillslopes
	
	- drainage network as basic organizing structure
	
	- REW size depends on spatial and temporal resolution for the representation
	of watershed response as well as available data
	
	- each REW as a single discrete point and the original watershed is
	substituted by an agglomeration of points
	
	- points connected with each other by main channel of each REW
	
	- REW divided into five sub-regions:
	
	 1. u-subregion: unsaturated zone
	
	 2. s-subregion: saturated zone
	
	 3. o-subregion: saturated overland flow
	
	 4. r-subregion: main channel reach of the REW
	
	 5. c-subregion: concentrated overland flow (network of channels,
	rills, gullies, ephemeral streams, Hortonian overland flow)
	
	- balance equations derived for all sub-regions
	
	- simplifications and jump conditions for thermodynamic exchange between
	different phases, between different subregions, and between REWs
	derived from balance equations and a set of assumptions
	
	- second law of thermodynamics included to derive constitutive relationships
	
	 - i.e. rate of entropy production has to be non-negative for the
	physical system under consideration},
  doi = {10.1016/S0309-1708(98)00012-8},
  file = {:Reggiani_etal_1998_watershed_thermodynamics_mass-momentum-energy.pdf:PDF},
  issn = {0309-1708},
