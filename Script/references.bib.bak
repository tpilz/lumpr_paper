@ARTICLE{Winter2001,
  author = {Winter, Thomas C.},
  title = {The concept of hydrologic landscapes},
  journal = {J. Am. Water Resour. As.},
  year = {2001},
  volume = {37},
  pages = {335--349},
  number = {2},
  abstract = {Aim:
	
	- put furth the concept of hydrologic landscapes for conceptualizing
	the movement of groundwater, surface water, and atmospheric water
	in different terrain types
	
	- dissecting landscapes into their fundamental hydrologic characteristics
	to beter understand water movements
	
	
	Motivation:
	
	- drainage basin as fundamental unit for surface water systems
	
	- aquifer systems fundamental unit for groundwater
	
	- framework needed that considers the complete hydrological system
	
	
	Hydrologic landscape:
	
	- main idea: complete hydrologic system interacts with a single, simple
	physiographic feature which is the basic building block of all hydrologic
	landscapes
	
	- physiographic feature = fundamental hydrologic landscape unit (FHLU):
	
	 1. landsurface form (upland -> slope -> lowland)
	
	 2. geologic framework
	
	 3. climatic setting
	
	- hydrologic system of a FHLU consists of the movement of:
	
	 1. surface water
	
	 2. groundwater
	
	 3. atmospheric water
	
	- assumptions:
	
	 - common patterns of water flows can be associated with variations
	or multiples of FHLUs
	
	 - water table reflects configuration of the land surface
	
	 - movement of water is controlled by common physical priciples regardless
	of geographic location
	
	- General hydrologic landscapes:
	
	 - variations and multiples of FHLU
	
	 - configurations of hydrologic landscapes
	
	 - describe major physiographic features of the earth
	
	
	Features:
	
	- hypotheses of hydrologic processes taking place can be developed
	and tested for any area regardless of scale
	
	- comparison of different study areas have a common foundation},
  doi = {10.1111/j.1752-1688.2001.tb00973.x},
  file = {:Winter_2001_concept_of_hydrologic_landscapes.pdf:PDF},
  issn = {1752-1688},
  keywords = {water resources geography, ground water hydrology, surface water hydrology,
	watershed management, geographical analysis, watershed systems},
  publisher = {Blackwell Publishing Ltd}
}

@ARTICLE{Wolock2004,
  author = {Wolock, M. David and Winter, C. Thomas and McMahon, Gerard},
  title = {Delineation and Evaluation of Hydrologic-Landscape Regions in the
	United States Using Geographic Information System Tools and Multivariate
	Statistical Analyses},
  journal = {Environ. Manage.},
  year = {2004},
  volume = {34},
  pages = {S71--S88},
  number = {1},
  abstract = {Hydrologic-landscape regions in the United States were delineated
	by using geographic information system (GIS) tools combined with
	principal components and cluster analyses. The GIS and statistical
	analyses were applied to land-surface form, geologic texture (permeability
	of the soil and bedrock), and climate variables that describe the
	physical and climatic setting of 43,931 small (approximately 200
	km2) watersheds in the United States. (The term ``watersheds'' is
	defined in this paper as the drainage areas of tributary streams,
	headwater streams, and stream segments lying between two confluences.)
	The analyses grouped the watersheds into 20 noncontiguous regions
	based on similarities in land-surface form, geologic texture, and
	climate characteristics. The percentage of explained variance (R-squared
	value) in an analysis of variance was used to compare the hydrologic-landscape
	regions to 19 square geometric regions and the 21 U.S. Environmental
	Protection Agency level-II ecoregions. Hydrologic-landscape regions
	generally were better than ecoregions at delineating regions of distinct
	land-surface form and geologic texture. Hydrologic-landscape regions
	and ecoregions were equally effective at defining regions in terms
	of climate, land cover, and water-quality characteristics. For about
	half of the landscape, climate, and water-quality characteristics,
	the R-squared values of square geometric regions were as high as
	hydrologic-landscape regions or ecoregions.},
  doi = {10.1007/s00267-003-5077-9},
  file = {:Wolock_etal_2004_hydro-landscape_units_GIS_statistics_USA.pdf:PDF},
  issn = {1432-1009}
}

@ARTICLE{Wood1988,
  author = {Eric F. Wood and M. Sivapalan and Keith Beven and Larry Band},
  title = {Effects of spatial variability and scale with implications to hydrologic
	modeling},
  journal = {J. Hydrol.},
  year = {1988},
  volume = {102},
  pages = {29--47},
  number = {1},
  abstract = {Research question: How does statistical behavior of runoff generation
	change with increases in catchment scale?
	
	
	Motivation
	
	- consistent theory for analyzing catchment responses at different
	scales
	
	- linking phenomena at field scales and catchment scales is an unresolved
	problem
	
	- runoff generation is a multiscale phenomenon with different length
	scales
	
	
	Conclusions
	
	- REA exists in the context of runoff generation response of catchments
	
	- REA strongly influenced by topography
	
	- soil and rainfall input variability of minor importance in determining
	the size of a REA
	
	
	Representative Elementary Area
	
	- spatially integrated representative catchment or assemblage of such
	representative subcatchments
	
	- averaging area acting as the smallest discernible point which is
	representative of the continuum is defined as REA
	
	- use of simple phenomenological equations to quantify runoff generation
	instead of differential equation formulations
	
	- required properties for a valid REA:
	
	 1. length scale of rapidly varying components << length scale of
	slowly varying components << length scale of the REA
	
	 2. average values obtained must be independent of the size of the
	REA
	
	 3. triplet of length scales (1.) must be constant for each field
	property and invariant in space and time
	
	 4. REA scale representation must contain all the physics of the sub-REA
	processes
	
	
	Model study
	
	- simulation approach to investigate existence of a REA
	
	- catchment: Coweeta River experimental catchment, North Carolina,
	USA
	
	- model: spatially distributed version of TOPMODEL
	
	
	Results
	
	- common threshold size for all studied outputs of 1km2
	
	- shape of natural subcatchments may affect the REA size
	
	- variability between subcatchment responses due to rainfall and soil
	variability
	
	- size of REA primarily governed by topography},
  doi = {10.1016/0022-1694(88)90090-X},
  file = {:Wood_etal_1988_representative_elementary_area_concept.pdf:PDF},
  issn = {0022-1694}
}

