@ARTICLE{Dehotin2008,
  author = {Dehotin, J. and Braud, I.},
  title = {Which spatial discretization for distributed hydrological models?
	Proposition of a methodology and illustration for medium to large-scale
	catchments},
  journal = {Hydrol. Earth Syst. Sc.},
  year = {2008},
  volume = {12},
  pages = {769--796},
  number = {3},
  abstract = {Aim:
	
	- catchment discretization based on flexible multi-scale hydrological
	framework
	
	- first level: sub-catchments, organized by river network
	
	- second level: homogeneous hydro-landscape units (based on geophysical
	factors and hydrological discontinuities)
	
	- third level: modelling units
	
	
	Motivation:
	
	- representation of landscape heterogeneity by:
	
	- distributed models:
	
	 - advantages: take landscape heterogeneity into account and provide
	distributed output
	
	 - disadvantages: for larger catchments simplifications are necessary,
	problems of data availability (often calibration needed), over parameterization,
	limitations in parameter estimation and validation
	
	- landscape discretization methodology considerations:
	
	 1. modelling objective
	
	 2. required output and spatial and temporal resolution
	
	 3. data availability and resolution
	
	 4. dominant hydrological processes and their functional scales
	
	 5. relevant process representation
	
	 6. acceptable degree of heterogeneity in modelling units
	
	- difficult to define a unique scale separating explicit and parameterized
	process representation
	
	 - due to hierarchical nature of river network and landscape complexity
	across scales
	
	 - characteristic scales for processes are different
	
	
	Hydro-landscape delineation:
	
	- two steps:
	
	 1. definition of homogeneous areas using available data
	
	 2. combination if first level sub-watersheds and other vector maps
	representing hydrological processes
	
	- homogeneous zones:
	
	 - small catcments: simple combination of available data
	
	 - larger catchments: simplifications based on classification techniques
	
	- homogeneous zone classification technique:
	
	 1. identification of available data and resolution
	
	 2. choice of representation of relevant processes
	
	 3. definition of factors considered influential on processes (e.g.
	slope, land use, geology, etc.)
	
	 4. simplification of each factor into classes
	
	 5. definition of reference zones (unique factor combination related
	to specific hydrological response)
	
	 6. choice of neighbouring window (size and shape) and descriptor
	(e.g. histogram, mean, standard deviation, etc.; applied to the window
	and to the reference zones)
	
	 7. mapping of the whole catchment by pixel-by-pixel analysis (i.e.
	assigning all pixels in the landscape the most similar reference
	zone based on descriptor)
	
	 8. distance map (distance between descriptor of each point and reference
	zone descriptor; e.g. modal distance, Kolmogorov distance, Manhatten
	distance; confidence of classification)
	
	 9. iteration of 6. to 8. until resolution ensures a given accuracy
	and consistency with input data (search for optimal window size and
	number of reference zones)
	
	- hydro-landscapes can be directly used for modelling or further discretized
	
	
	Modelling unit derivation:
	
	- deriving convex meshes based on hydro-landscapes
	
	- e.g. by trangulated irregular networks or specific algorithms},
  doi = {10.5194/hess-12-769-2008},
  file = {:Dehotin_Braud_2008_landscape_discretization_large-scales.pdf:PDF}
}

