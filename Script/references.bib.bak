@ARTICLE{Nijzink2016,
  author = {Nijzink, R. C. and Samaniego, L. and Mai, J. and Kumar, R. and Thober,
	S. and Zink, M. and Sch\"afer, D. and Savenije, H. H. G. and Hrachowitz,
	M.},
  title = {The importance of topography-controlled sub-grid process heterogeneity
	and semi-quantitative prior constraints in distributed hydrological
	models},
  journal = {Hydrol. Earth Syst. Sc.},
  year = {2016},
  volume = {20},
  pages = {1151--1176},
  number = {3},
  abstract = {Hypotheses: \par
	
	1. topography-derived response units representing sub-grid variability
	improve model internal dynamics and predictive power \par
	
	2. semi-quantitative expert-based model constraints improve identifiability
	of model parameters \par
	
	3. combined use of response units and model constraints improves spatial
	transferability \par
	
	
	Background: \par
	
	- better understanding of the link between landscape heterogeneity
	and process dynamics is required \par
	
	- lumped representations of catchments fail to adequately represent
	dominant features of hydrological response at the catchment scale
	\par
	
	- (semi-)distributed models have shown to alleviate oversimplification
	by accommodating spatial heterogeneity \par
	
	- limitations of traditional conceptual approaches: \par
	
	 - constrained by grid size of available data or size of defined subcatchments
	\par
	
	 - mostly the defined processes are the same among individual model
	units \par
	
	 - sub-grid process heterogeneity merely reflected by distinction
	functions or constitutive relationships but still no generally apllicable
	closure relations found \par
	
	- topographic data to derive response units within a semi-distributed
	modeling approach accounting for process heterogeneity can improve
	model skill \par
	
	- large parameter space of distributed model approaches results in
	poor parameter identifiability and equifinality \par
	
	- methods to constrain the feasible model / parameter space and improve
	identifiability:\par
	
	 - model regularization relying on relations between catchment characteristics
	and model parameters \par
	
	 - expert-based semi-quantitative information on catchment functioning
	("soft data") \par
	
	
	Study areas and data: \par
	
	- 7 catchments in central and wester Europe with different climatological,
	geographical and geological conditions \par
	
	- daily discharge (GRDC) and meteorological (E-OBS) data \par
	
	
	Modelling: \par
	
	- mesoscale hydrological model (mHM) \par
	
	 - distributed, process-based \par
	
	- topography-driven mHM \par
	
	 - explicit representation of landscape classes defined by HAND approach
	and slope: plateau, hillslopes, and wetland \par
	
	 - different model structures for the classes run in parallel, connected
	by common groundwater storage \par
	
	- parameter regionalization \par
	
	 - global parameters defining functional relationships between model
	parameters and catchment characteristics; can be transferred to similar
	catchments without calibration \par
	
	 - multiscale parameter regionalization (MPR): model parameters at
	input data resolution (inferred by calibration of global parameters)
	need to be upscaled to model resolution; method depends on considered
	parameter; global parameter and upscaling for each landscape class
	individually \par
	
	- expert-based constraints: \par
	
	 - constraints for runoff coefficients taking into account the whole
	time series, the high flow period, and the low flow period; period
	defined from expert knowledge about the catchment \par
	
	 - soil moisture storage capacity constraints \par
	
	- calibration: \par
	
	 - for each catchment using MC sampling approach and four objective
	functions \par
	
	- post-calibration evaluation \par
	
	 - model skill assessed by reproducing a variety of observed hydrological
	signatures \par
	
	
	Results: \par
	
	- similar calibration performance \par
	
	- sub-grid process heterogeneity did not show a clear pattern of improvement
	or deterioration \par
	
	- constraints helped to increase model performance and reduce model
	uncertainty \par
	
	- sub-grid heterogeneity and constraints improved transferability
	to other catchments without calibration \par},
  doi = {10.5194/hess-20-1151-2016},
  file = {:Nijzink_etal_2016_sub-grid_processes_prior_constrains_hydrol_models.pdf:PDF}
}

@ARTICLE{Noel2014,
  author = {P. Noël and A. N. Rousseau and C. Paniconi and D. F. Nadeau},
  title = {Algorithm for Delineating and Extracting Hillslopes and Hillslope
	Width Functions from Gridded Elevation Data},
  journal = {J. Hydrol. Eng.},
  year = {2014},
  volume = {19},
  pages = {366--374},
  number = {2},
  abstract = {Aim:
	
	- methodology for delineating hillslopes and extracting width functions
	from digital terrain data
	
	
	Motivation:
	
	- hillslope as most common unit for catchment subdivision
	
	- hillslope width functions (HWF) play central role in hillslope-based
	models
	
	- several terrain analysis algorithms exist but less and no standardized
	metods for hillslope delineation
	
	
	Method:
	
	1) hillslope delineation:
	
	 - DEM
	
	 - flow direction calculation (steepest descent)
	
	 - flow accumulation
	
	 - river network extraction
	
	 - subdivision into subbasins
	
	 - algorithm for extraction of hillslopes (distinguish between headerwater
	and lateral hillslopes, i.e. subbasin consists of river segment and
	two or three hillslopes)
	
	2) characterization of plan shape and profile curvature
	
	 - plan shape calculated from DEM:
	
	 - tangential curvature perpendicular to the slope gradient
	
	 - elevation line can be convergent, divergent, or uniform
	
	 - calculation of convergence ratio for every hillslope
	
	 - profile curvature: 
	
	 - refers to rate of change of slope
	
	 - elevation line can be concave, convex, or straight
	
	 - calculation of convexity ratio for every hillslope
	
	 - convergence and convexity ratios used in fuzzy logic algorithm
	for classification into nine elementary landform classes of Dikau
	
	3) Hillslope Width Function (HWF)
	
	 - criteria: 1) monotonicity to avoid flow singularities; 2) conservation
	of surface area for mass conservation in the model
	
	 - different algorithms for lateral (resulting in rectangular form)
	and headwater hillslopes (resulting in triangular form)
	
	 - for each hillslope the distance from river segment to the divide
	and the width at the divide are calculated
	
	
	Conclusions:
	
	- pre-processing algorithm for use in catchment models using hillslopes
	as computational elements
	
	- some difficulty in flat catchments
	
	- relatively quick and flexible
	
	- particular useful for hillslope-storage Boussinesq model},
  doi = {10.1061/(ASCE)HE.1943-5584.0000783},
  eprint = { http://dx.doi.org/10.1061/(ASCE)HE.1943-5584.0000783 },
  file = {:Noel_etal_2013_hillslope_delineation_from_DEM.pdf:PDF}
}

