@ARTICLE{Sivakumar2015,
  author = {Bellie Sivakumar and Vijay P. Singh and Ronny Berndtsson and Shakera
	K. Khan},
  title = {Catchment Classification Framework in Hydrology: Challenges and Directions},
  journal = {J. Hydrol. Eng.},
  year = {2015},
  volume = {20},
  pages = {A4014002},
  number = {1},
  abstract = {Aim:
	
	- discuss major challenges of current approaches in developing a generic
	catchment classification framework
	
	- formulation of a classification framework
	
	
	Motivation:
	
	- advances in hydrology and water resources but still many challenges:
	
	 1. models too complex, too many parameters, require too much data
	
	 2. model extension and generalization often difficult
	
	 3. lack of emphasis on connection between model theory and system
	properties
	
	 4. too much emphasis on specific concepts and techniques rather than
	coupling and integration
	
	 5. focus rather on local scale problems than global scale issues
	
	- need for model simplification and a common modelling framework
	
	- some advances already facing challenges
	
	- existing catchment classifications based on river morphology, flow
	regimes, climate, landscape, land use, hydrologic similarity, hydrologic
	signatures, ecohydrology, hydropedology, geostatistics, entropy,
	scalings, nonlinear dynamics, data mining, etc.
	
	- lack of coherent effort to bring all these form together
	
	
	Classification framework:
	
	- modeling approaches: treat all catchments the same vs. treat each
	catchment individually
	
	 - framework must provide an optimum way between both
	
	- should accommodate general and specific characteristics
	
	- must be simple and generally agreeable
	
	- catchment complexity: external (e.g. climate) + internal (e.g. landscape
	properties) factors and processes occurring over a wide range of
	spatial and temporal scales
	
	- reductionist or bottom-up approach: breaking catchment into smaller
	domains and studying smaller domains in detail
	
	- system approach: different definitions of a system and how to define,
	qualify and quantify complexity of systems},
  doi = {10.1061/(ASCE)HE.1943-5584.0000837},
  eprint = { http://dx.doi.org/10.1061/(ASCE)HE.1943-5584.0000837 },
  file = {:Sivakumar_etal_2015_catchment_classification_framework_challenges.pdf:PDF}
}

@ARTICLE{Smith2013,
  author = {Smith, Tyler and Marshall, Lucy and McGlynn, Brian and Jencso, Kelsey},
  title = {Using field data to inform and evaluate a new model of catchment
	hydrologic connectivity},
  journal = {Water Resour. Res.},
  year = {2013},
  volume = {49},
  pages = {6834--6846},
  number = {10},
  abstract = {Aim:
	
	- introduction of the Catchment Connectivity Model (CCM) as a framework
	emphasizing external and internal hydrologic interpretation and prediction
	using landscape structure (topography) as basic building block
	
	
	Objective:
	
	- hydrologic connectivity definition:
	
	 "... the water-mediated transport of matter, energy, and organisms
	within or between elements of the landscape that can be unidirectional
	or bidirectional and temporally variable"
	
	- test hydrologic connectivity concept:
	
	 1. can the concept be implemented in a simple, spatially explicit
	hydrologic model?
	
	 2. can it be used to simulate observed streamflow dynamics?
	
	 3. can the model yield internally consistent dynamics without parameter
	fit?
	
	
	Motivation:
	
	- comlex process-based vs. simple conceptual models with different
	advantages and shortcommings
	
	- desire for models that combine advantages: providing efficient,
	process-based hypotheses about streamflow generation
	
	- critical to determine:
	
	 1. factors affecting hydrologic functioning
	
	 2. relative importance of factors to select model structures a priori
	
	 3. variation across different catchment types
	
	- problem: lack of coordination between experimentalists and modellers
	
	- challenge: quantification of relationship between catchment form
	and hydrologic functioning required
	
	
	Catchment Connectivity Model:
	
	- hypothesis: frequency of landscape-scale hydrologic connectivity
	determines catchment runoff magnitude
	
	- primary components: 
	
	 1. landscape analysis: delineation of hillslopes based on upslope
	accumulated area derived from DEM 
	
	 2. soil moisture accounting: each hillslope has own moisture storage
	(precip - aet - outflow)
	
	 3. hydrologic connectivity estimation: based on storage threshold
	function defining critical volume for a hillslope to become hydrologically
	connected and contribute discharge to the stream
	
	- frequency-based rather than magnitude-based connectivity: each hillslope
	has the same rate of release if connected
	
	
	Features:
	
	- simple conceptual hydrologic model
	
	- requires calibration but is physically-based and parameters are
	physically interpretable
	
	- combines strengths of classical conceptual and physically-based
	models
	
	- for shallow catchments with topographically driven connectivity
	as dominant process
	
	- model development based on field observations rather than perceived
	process understanding
	
	
	Limitations:
	
	- dominant process concept results in exclusion of secondary processes
	
	- constant outflow rate assumption for all connected hillslopes; no
	variation in magnitude of throughflow which at least might play a
	minor role
	
	- losses and gains between streams and groundwater not included
	
	- assumes insignificant overland flow
	
	- contributing area of individual hillslopes neglected in contrary
	to traditional storage approaches
	
	- model parameters could not be estimated a priori despite their physical
	interpretability
	
	
	Performance:
	
	- strong correspondence between measured and modelled data
	
	- minor disagreement connectivity due to: 
	
	 - deep groundwater contribution at low flows
	
	 - potential errors in model structure and assumptions
	
	 - model does not take into account spatial or temporal variability
	of factors
	
	 - model driving data may not be fully representative
	
	- model generally underestimated peak flows
	
	- good agreement in wet-up and recession periods},
  doi = {10.1002/wrcr.20546},
  file = {:Smith_etal_2013_model_of_catchment_hydrologic_connectivity.pdf:PDF},
  issn = {1944-7973},
  keywords = {Modeling, Catchment, Geomorphology: hillslope, Watershed, riparian,
	hillslope, topography, landscape analysis, hydrologic connectivity,
	conceptual model, source areas, runoff, watershed, catchment}
}

@ARTICLE{Steiniger2009,
  author = {Stefan Steiniger and Erwan Bocher},
  title = {An overview on current free and open source desktop GIS developments},
  journal = {Int. J. Geogr. Inf. Sci.},
  year = {2009},
  volume = {23},
  pages = {1345--1370},
  number = {10},
  abstract = {- Background:
	
	 - Variety of Free and Open Source Software (FOSS):
	
	 - word processors
	
	 - web browsers
	
	 - drawing
	
	 - scientific applications
	
	 - GIS
	
	 - popularity measures:
	
	 - number of projects started (increasing)
	
	 - increasing financial support
	
	 - download rates of software
	
	 - documented number of use cases of FOSS
	
	 - FOSS movement:
	
	 - freedom of use and modification of software
	
	 - also includes free availability of data
	
	 - facilitaed by (online) platforms
	
	- three domains:
	
	 - 'free' software: freedom to ...
	
	 1. run the program for any purpose
	
	 2. study how the program works and adapt it to your needs
	
	 3. redistribute copies
	
	 4. improve the program and release improvements to the public
	
	 - no restrictions whether improved software has to be offered for
	free or can be sold
	
	 - 2. and 4. imply that source code has to be delivered with the software
	
	 - defined by free software foundation (FSF)
	
	 - 'proprietary' software:
	
	 - closed software: non-modifiable software (opposed to freedom 2)
	
	 - shareware: software free for download and use, usually do not allow
	study and modification; 'free' software in terms of 'free beer'
	
	 - 'open source' software:
	
	 - source code accessible
	
	 - source code can be studied
	
	 - no freedom of modification and redistribution
	
	 - term introduced by open source initiative (OSI); properties of
	initiative not exactly compatible with free software definition (rather
	a philosophical difference)
	
	 - 'free and open source' software (FOSS): in this article FSF and
	OSI certified license
	
	- licenses
	
	 - several FOSS licenses with different degrees of protection of the
	four freedoms
	
	 - most protective license guaranteeing for all four freedoms is the
	GPL
	
	 - 'copyleft': derived or modified software and extracted code must
	be covered by the same license
	
	 - copyleft problems if free software should be coupled with porprietary
	software: LGPL to avoid problems
	
	 - common GPL misconceptions:
	
	 1. only a release of the software requires publishing the source
	code; code can be kept private and licensed GPL as long as the software
	has not been distributed to the public
	
	 2. copies of the software can be sold and download fees be raised
	
	 3. software created with GPL software is not affected by GPL (only
	if it contains GPL licensed code sniplets)
	
	- GIS software world:
	
	 - open source GIS organizations
	
	 - open source geospatial foundation (OSGeo): hosts software projects,
	publishes a journal, founded education and curriculum committee,
	organizes conferences, donates an award
	
	 - open geospatial consortium (OGC): develops standards for processing
	and exchange of geodata (also for proprietary software)
	
	 - software categories forming spatial data infrastructures (SDI):
	
	 - software to collect, prepare, analyse data (desktop GIS)
	
	 - databases to store data
	
	 - web server and client software to deliver data online
	
	 - software libraries, third party plug-ins or software extensions
	providing specific functionalities
	
	 - grouping of projects
	
	 - based on development platform (e.g. using C++, JAVA, or Python,
	etc.)
	
	 - foundation and maintenance (commercial vs. enthusiasts vs. research
	projects at universities or other oagnizations vs. support from governmental
	agencies)
	
	- major FOSS desktop GIS projects
	
	 - desktop GIS: runs on PC, allows user to display, query, update,
	and analyse data
	
	 - evaluation of several FOS GIS products (GRASS, QGIS, MapWindow,
	etc.)
	
	- FOS vs. proprietary GIS
	
	 - advantages FOS:
	
	 - no fees to pay
	
	 - support of standard and commonly used formats
	
	 - unrestricted use
	
	 - support from several providers
	
	 - disadvantages FOS:
	
	 - training costs for users will emerge
	
	 - limited availability and consistency of documentation
	
	 - sometimes problems with continuity and long-term planning
	
	 - disadvantages proprietary:
	
	 - software price and maintenance fees
	
	 - training costs
	
	 - potential loss of support for certain functionalities which is
	unlikely for FOS
	
	 - license changes may occur affecting users
	
	 - support only as long as the company exists
	
	 - advantages proprietary:
	
	 - software components should work well together
	
	 - usually well documented
	
	 - FOS in teaching:
	
	 - furthering development of a critical perspective on GIS
	
	 - experiment with the software at home without having to pay fees
	
	 - source code and algorithms can be explored
	
	 - potential lack in functionality
	
	 - users could be biased against companies or organisations looking
	for people with know how on specific porprietary software
	
	 - FOS in research:
	
	 - preserves researcher from 're-inventing the wheel'
	
	 - studying source code (not only a written description of the algorithm)
	is possible
	
	 - software adaptable to own research needs
	
	 - new developments available in-time, no wait for new software releases
	
	 - unlimited software portability},
  doi = {10.1080/13658810802634956},
  eprint = { http://dx.doi.org/10.1080/13658810802634956 },
  file = {:Steiniger_Bocher_2009_overview_open-source_GIS.pdf:PDF}
}

@ARTICLE{Strahler1957,
  author = {Strahler, Arthur N.},
  title = {Quantitative analysis of watershed geomorphology},
  journal = {Eos, Transactions American Geophysical Union},
  year = {1957},
  volume = {38},
  pages = {913--920},
  number = {6},
  abstract = {Aim:
	
	- review of progress made in quantitative landform analysis
	
	
	Quantitative geomorphology:
	
	- based on dimensional analysis and principled of scale-model similarity
	
	- all measurements of length between corresponding points in two basins
	bear a fixed scale ratio
	
	- in two geometrically similar systems all dimensionless numbers or
	products describing geometry must be equal (e.g. angles)
	
	- in geologic homogeneous systems geometric similarity is approximated
	when mean values are considered
	
	
	Drainage basin analysis:
	
	1) order analysis
	
	- Strahler-order of rivers: finger-tip tributaries order one; two
	first-order channels join to order two; two second-order channels
	joint to order three; etc.
	
	- premise: on average order number is directly proportional to relative
	watershed dimensions, channel size, and stream discharge
	
	- order is dimensionless: two drainage basins can be equated or compared
	with respect to corresponding points in their geometry through use
	of order number
	
	2) bifurcation ratio
	
	- Horton's law of stream numbers: numbers of stream segments of each
	order from an inverse geometric sequence with order number
	
	- bifurcation ratio r: anti-logarithm of regression coefficient when
	plotting sream order (abcissa) vs. log of stream segment numbers
	(ordinate)
	
	- i.e. on average there are r times as many streams of one order as
	of the next higher order
	
	- r shows little variation from region to region except where powerful
	geologic controsl dominate
	
	3) distribution of stream lengths
	
	4) relation of sream length to stream order
	
	- power function can be used to relate two variables
	
	- marked differences in exponent of relationship indicate it being
	a useful measure of changing length of channel segments as the order
	changes
	
	5) drainage basin area
	
	- for meaningful analysis it is necessary to compare basins of the
	same order of magnitude
	
	- basin area increases exponentially with stream order
	
	6) drainage density and texture ratio
	
	- drainage density: 
	
	 - sum of channel lengths divided by basin area
	
	 - important indicator of linear scale of landform elements
	
	 - size of drainage units decreases with increasing drainage density
	
	 - wirde ratio of variation
	
	- texture ratio:
	
	 - number of contour crenulations divided by length of perimeter
	
	- relationship between both characterizes texture of basin
	
	7) constant of channel maintenance
	
	- c: inverse of drainage density
	
	- i.e. on the average c of surface are required to maintain a unit
	of channel length
	
	- means of indicating relative size of landform units in a drainage
	basin
	
	8) maximum valley side slopes
	
	- within-area variance small compared to between-area differences
	
	- valuable measure relating closely to sediment production
	
	9) mean slope curve
	
	- mean belt width: area of each contour belt divided by length of
	contour belt
	
	- contour interval divided by mean belt width
	
	- depends on geologic structure and stage of development of the drainage
	system
	
	- mean slope for basin: area-weighted average of all mean slopes of
	contour belts
	
	10) slope maps
	
	11) rapid slope sampling
	
	- random point sampling
	
	12) relief ratio
	
	- ratio between total basin relief (elevation different between basin
	mouth and summit) and basin length (longest dimension of drainage
	basin)
	
	- useful for estimation of sediment yield
	
	13) hypsometric analysis
	
	- relation of horizontal cross-sectional drainage basin area to elevation
	
	- several dimensionless attributes of curve are measureable an can
	be used for comparative analysis
	
	- curve properties tend to be stable in homogeneous rock masses and
	to adhere to the same curve family for given geologic and climatic
	combination},
  doi = {10.1029/TR038i006p00913},
  file = {:Strahler_1957_analysis_of_watershed_geomorphology.pdf:PDF},
  issn = {2324-9250}
}

