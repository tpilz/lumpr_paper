@ARTICLE{Zehe2014,
  author = {Zehe, E. and Ehret, U. and Pfister, L. and Blume, T. and Schr\"oder,
	B. and Westhoff, M. and Jackisch, C. and Schymanski, S. J. and Weiler,
	M. and Schulz, K. and Allroggen, N. and Tronicke, J. and van Schaik,
	L. and Dietrich, P. and Scherer, U. and Eccard, J. and Wulfmeyer,
	V. and Kleidon, A.},
  title = {HESS Opinions: From response units to functional units: a thermodynamic
	reinterpretation of the HRU concept to link spatial organization
	and functioning of intermediate scale catchments},
  journal = {Hydrol. Earth Syst. Sc.},
  year = {2014},
  volume = {18},
  pages = {4635--4655},
  number = {11},
  abstract = {- 30 years ago: complexity of intermediate-scale catchments (5 to
	250 km2) is a cardinal problem in hydrology \par
	
	 - too large for fully deterministic treatment \par
	
	 - too small for simplified conceptual treatment \par
	
	- today still a gap at intermediate scale with respect to:\par
	
	 - understanding \par
	
	 - structurally adequate models \par
	
	 - experimental strategies to collect useful data \par
	
	
	Aim: better understanding of how different forms of spatial organization
	affect storage and release of water and energy across scales \par
	
	- key: reinterpretation of the concept of hydrological response units
	from a thermodynamic perspective \par
	
	- proposing alternatives for: \par
	
	- defining specific functional units for a specific form of water
	release instead of a static "one fits all processes" HRU \par
	
	- experimental strategies to characterize different forms of spatial
	organization \par
	
	- structural adequate models allowing equifinality and methods to
	reduce equifinality \par
	
	- assessing accordance of persistent spatial organization with thermodynamics
	\par
	
	
	Thermodynamic perspective: \par
	
	- second law of thermodynamics: \par
	
	 - (potential) gradients are depleted by fluxes that are caused by
	these gradients \par
	
	 - implies production of entropy and dissipation of free energy (i.e.
	the capacity of a system to perform work) \par
	
	 - flux = conductance (inverse of resistance) * gradient \par
	
	 - magnitude of fluxes controlled by: governing equations, relevant
	resistances, thermodynamic limits \par
	
	- isolated systems: \par
	
	 - exchange neither mass nor energy \par
	
	 - evolve to a dead state of maximum entropy, i.e. a thermodynamic
	equilibrium \par
	
	- open systems: \par
	
	 - export entropy to the environment \par
	
	 - maintain a spatial configuration far from equilibrium \par
	
	- two forms of water release from a catchment: \par
	
	 - water vapor to the atmosphere by evapotranspiration \par
	
	 - driven by radiative heating \par
	
	 - plants as preferential flow paths for water from the soil \par
	
	 - entropy production largely dominated by ET \par
	
	 - entropy export by outgoing long-wave radiation and turbulent heat
	fluxes \par
	
	 - streamflow \par
	
	 - driven by gravity \par
	
	 - max fluxes linked to conversion of free energy (chemical energy,
	pot. and kin. energy of soil or surface water) \par
	
	 - energy conversion related to partition of rainfall to runoff components
	and storage dynamics \par
	
	 - entropy production by energy conversion \par
	
	
	Landscape organization \par
	
	- spatial organization affects gradients and resistances \par
	
	- persistence of topographic gradients most obvious form of organization
	\par
	
	- omnipresence of networks of preferential flow paths as example of
	spatial organization \par
	
	- idea of HRU implies that landscape evolution creates spatial organization,
	reflected in similar hydrologial behaviour \par
	
	- might be a result of co-evolution of distinct natural communities,
	landscape characteristics, and suitable management practices \par
	
	- structural similarity might imply that past process patterns and
	human disturbances have been similar \par
	
	
	Propose of three specific functional units: \par
	
	1. Elementary functional units (EFU): \par
	
	- similar terrestrial controls on surface energy balance \par
	
	- control functional similarity during radiation-driven conditions
	\par
	
	- represented by the same parameter set related to energy balance
	and vertical flows \par
	
	- first guess predictors: land use, management practice, location
	within catena, hillslope aspect \par
	
	2. Lateral topological units (LTU): \par
	
	- similar terrestrial controls on rainfall runoff generation \par
	
	- control functional similarity \par
	
	- represented by same parameter set characterizing lateral flows and
	EFU-scale parameters \par
	
	- first guess predictors: surface and bedrock topography and morphology,
	land use \par 
	
	3. Subcatchment scale hydro-geomorphic units (HGU) \par
	
	- headwater or subcatchment scale \par
	
	- homogeneity with respect to groundwater storage and release \par
	
	- determined by geology, land use, climatic setting \par
	
	
	Structural adequate modelling \par
	
	- should be thermodynamically consistent (see above) \par
	
	 - allows constraining behavioral flow resistances by available information
	\par
	
	 - to determine parameters in a practical feasible manner structural
	and functional parameter sets must be transferable among members
	of the same EFU or LTU \par
	
	- separate treatment of fluxes in matrix/continuum elements and connected
	network-like structures \par
	
	 - because different forms of fluxes (matrix flow vs. preferential
	flow) dominated by different forces and deplete different gradients
	and are independent sources of equifinality \par
	
	- balances necessary complexity with the greatest possible parsimony
	\par
	
	 - key factors: spatial organisation, preferential flow paths and
	how to couple fast and slow flow domains, conceptualization of driving
	gradients \par
	
	- existing approaches: \par
	
	 - representative elementary watershed (REW) \par
	
	 - hillslope storage Boussinesq (HSB) \par
	
	 - conceptualizations of landscape characteristics (e.g. TOPMODEL,
	WASA, mHm) \par
	
	 - common most severe limitations: too simple descriptions of land-surface
	energy exchange and shallow turbulence parameterizations \par
	
	
	Catchment as organized systems (CAOS) model \par
	
	- thermodynamically consistent \par
	
	- disentangles matrix and preferential flow \par
	
	- parsimony by representing only dominant flow processes at EFU, hillslope
	and catchment levels in a coupled 1D manner \par
	
	- EFU: 
	
	 - vertical flows of land-surface energy exchange and ET (Penman-Monteith)
	
	 - vertical flows during radiation-driven conditions (capillary rise,
	soil heat): Darcy-Richards equation
	
	 - vertical flows during gravity-driven conditions (preferential macropore
	flow): kinematic wave or stochastic approach \par
	
	 - water beyond saturation: directed to macropore domain or Rapid
	Subsurface Flow object connecting EFUs along downslope gradient \par
	
	 - lower boundary conditions: free drainage to slow groundwater flow
	\par
	
	- lateral exchange by overland flow or subsurface lateral flow by
	diffusion wave or Darcy-Weisbach equation \par
	
	- base flow through diffusion wave equation \par
	
	- stream domain represented as network, described by kinematic wave
	equations \par
	
	- advection-dispersion equation to simulate transport of solutes,
	isotopes, and thermal energy \par
	
	- numerical integration via adaptive time stepping and explicit/implicit
	Crank-Nicholson scheme \par},
  doi = {10.5194/hess-18-4635-2014},
  file = {:Zehe_etal_2014_response_units_to_functional_units.pdf:PDF}
}

