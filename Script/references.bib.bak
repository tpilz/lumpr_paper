@ARTICLE{Reggiani1999,
  author = {Paolo Reggiani and S.Majid Hassanizadeh and Murugesu Sivapalan and
	William G. Gray},
  title = {A unifying framework for watershed thermodynamics: constitutive relationships},
  journal = {Adv. Water Resour.},
  year = {1999},
  volume = {23},
  pages = {15--39},
  number = {1},
  abstract = {- sequel paper of Reggiani et al. (1998)
	
	
	Aim:
	
	- resolve the closure problem of REW's ODEs by exploiting the second
	law of thermodynamics (Coleman and Noll method)
	
	
	Motivation:
	
	- for the REW a set of ODEs was defined averaging sub-REW variability
	
	- during averaging a series of exchange terms have been defined
	
	- terms are unknowns of the problem
	
	- number of unknowns is greater than the number of equations
	
	- an appropriate closure scheme needs to be defined which leads to
	the derivation of constitutive relationships for the unknown exchange
	terms
	
	
	Thermodynamic background:
	
	- second law of thermodynamics: inequality representing total entropy
	production of a system
	
	- absolute minimum of entropy production is always seen to hold under
	thermodynamic equilibrium conditions; i.e. entropy production is
	zero
	
