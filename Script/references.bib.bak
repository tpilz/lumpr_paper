  file = {:Olden_etal_2012_hydrologic_classification_methods_applications.pdf:PDF},
  issn = {1936-0592},
  keywords = {dams, flow regime, environmental flow, river regulation, hydrologic
	metric},
  publisher = {John Wiley \& Sons, Ltd}
}

