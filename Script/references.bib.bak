@ARTICLE{Fenicia2016,
  author = {Fenicia, Fabrizio and Kavetski, Dmitri and Savenije, Hubert H. G.
	and Pfister, Laurent},
  title = {From spatially variable streamflow to distributed hydrological models:
	Analysis of key modeling decisions},
  journal = {Water Resour. Res.},
  year = {2016},
  volume = {52},
  pages = {954--989},
  number = {2},
  abstract = {Objective: \par
	
	- review and assess hydrological hypotheses associated with three
	key decisions in distributed modelling (see below) \par
	
	
	Study area: \par
	
	- Attert catchment in Luxembourg with subcatchment with markedly different
	streamflow dynamics \par
	
	
	Hypotheses: \par
	
	1. geology controls dominant runoff generating processes \par
	
	2. alternative hypothesis: dominant runoff processes are controlled
	by topography \par
	
	3. null hypothesis: neither geology nor topography are active controls
	\par
	
	
	Conclusions: \par
	
	- differences in streamflow resonse in the catchment related to geology
	instead of topography which is usually assumed a priori in most hydrological
	model applications \par
	
	- simple model structures tailored to specific HRUs perform better
	than usung a general complex model structure \par
	
	- simpler model structures in general have a better parameter identifiability
	even if they perfom equally to more complex models \par
	
	- landscape classification approach has a strong impact on model results
	\par
	
	
	Review: \par
	
	Distributed catchment models \par
	
	- represent spatial heterogeneity of hydrological processes \par
	
	- bottom-up philosophy: \par
	
	 - system behaviour characterized by exlicit and detailed representation
	of processes and interactions
	
	 - referred to as "physical" models \par
	
	 - parameters and states are measurable \par
	
	 - limitations: large data requirements and computational costs, not
	necessarily represent best conceptual tools \par
	
	 - representative elementary watershed (REW) approach: upscale balance
	equations from point to catchment scale \par
	
	 - limitation: appropriate averaging volumes and closure relationships
	at landscape scale remain challenges \par
	
	- top-down philosohy: \par
	
	 - parsimonious description of emergent properties of the entire system
	\par
	
	 - referred to as "conceptual" models \par
	
	 - abstract description where states and parameters may not correspond
	to directly observable quantities \par
	
	 - calibration (inverse modelling) techniques are needed to estimate
	parameters \par
	
	- in practice mostly blurred than clear-cut classifications \par
	
	Decisions in distributed hydrological models: \par
	
	1. spatial discretizations \par
	
	 - regular grids \par
	
	 - irregular networks \par
	
	 - hillslopes \par
	
	 - subcatchments \par
	
	 - hydrological response units (HRU) \par 
	
	 2. structure and connectivity of resulting spatial elements \par
	
	 - apply the same structure across the entire domain or taior model
	structures at spatial model element scale based on dominant processes
	\par
	
	 - elements may be fully connected by lateral flows or connected through
	groundwater or discharge into a common stream \par
	
	 3. specification of structure and state constraints \par
	
	 - regularization procedures to constrain model parameters and avoid
	overparameterization \par
	
	 - calibration focus on parameters of regional relationships between
	parameters or to watershed characteristics instead of parameters
	of the individual model elements \par
	
	Multiple working hypotheses: \par
	
	- model decisions seldom unique and represent multiple working hypotheses
	that need to be tested \par
	
	- particularly powerful within flexible modelling frameworks \par
	
	
	Catchment: \par
	
	- Attert catchment Luxembourg \par
	
	- geological landscape classifications: \par
	
	 - based on three main bedrock types: schist, marl, sandstone, alluvial
	deposits \par
	
	- topographical classifications: \par
	
	 - classification based on slope and "height above nearest drainage"
	(HAND) \par
	
	 - classes: wetland, hillslope, plateau \par
	
	
	Landscape discretization: \par
	
	- catchment -> sub-catchments (same forcing) -> HRUs based on geology
	or topography (same model structure and parameters) -> fields (specific
	model structure and forcing) \par
	
	- connectivity: fields are laterally disconnected; distinguished routing
	of streamflow from internal fields versus streamflow from upstream
	inlets \par
	
	
	Model structures: \par
	
	- lumped model structures \par
	
	- calibration by Bayesian Weighted Least Squares approach und uncertainties
	estimated using MCMC sampling algorithm \par
	
	
	Multiple hypotheses framework: \par
	
	- two approaches: \par
	
	 1. models generated a priori to cover the entire space of plausible
	hypotheses \par
	
	 2. models generated, trialed, and modified iteratively -> used in
	this study as first approach hardly feasible in distributed modelling
	\par
	
	- models generated according to three key decisions (see above) \par
	
	- hypotheses compared based on streamflow predictive ability using
	split-sample validation and multiple metrics \par
	
	 - time transferability validated by subdividing observation period
	into two periods (temporal split-sample) \par
	
	 - space transferability estimated by gouping subcatchments into two
	groups (spatial split-sample) \par
	
	
	Decision 1: landscape hypotheses \par
	
	 1. entire cacment a single HRU \par
	
	 2. topography-based discretization \par
	
	 3. geology-based discretization \par
	
	 - the same model structure for all discretizations: three reservoirs
	and one lag function
	
	 - results: \par
	
	 - geology-based discretization performs best in reproducing different
	streamflow dynamics \par
	
	 - worst in terms of identifiability of model parameters as it is
	the most complex \par
	
	 - limited space transferability but good temporal transferability
	\par
	
	
	Decision 2: model structural hypotheses \par
	
	 - basis of analysis is geology-based landscae discretization \par
	
	 - for each HRU (i.e. geology class) a specific conceptual model structure
	is tailored based on the observable characteristic streamflow dynamics
	\par
	
	 - in total, the model structural comlexity was reduced (less total
	number of states and parameters) \par
	
	 - results: \par
	
	 - minor reduction in performance when models where calibrated to
	headwater subcatchments; major improvement when model where calibrated
	to downstream subcatchments \par
	
	 - improved space transferability while temporal transferability remained
	stable \par
	
	 - better performance than the generic model (except during calibration
	but might be result of overfitting of the complex generic structure)
	\par
	
	 - better parameter identifiability \par
	
	
	Decision 3: spatial relations between model parameters across HRUs
	\par
	
	 - basis of analysis is tailored model structure of Decision 2 and
	geology-based landscae discretization \par
	
	 - further reduction of complexity by using the same model parameters
	across all HRUs \par
	
	 - results: virtually identical model performance, better parameter
	identifiability, direct support for constraining parameter \par},
  doi = {10.1002/2015WR017398},
  file = {:Fenicia_etal_2016_analysis_of_modeling_decisions.pdf:PDF},
  issn = {1944-7973},
  keywords = {Catchment, Modeling, Streamflow, Distributed models, streamflow, geology,
	topography, hypothesis testing, HRU, SUPERFLEX}
}

