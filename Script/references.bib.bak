@ARTICLE{Haverkamp2002,
  author = {Haverkamp, S. and Srinivasan, R. and Frede, H. G. and Santhi, C.},
  title = {Subwatershed spatial analysis tool: discretization of a distributed
	hydrologic model by statistical criteria},
  journal = {J. Am. Water Resour. As.},
  year = {2002},
  volume = {38},
  pages = {1723--1733},
  number = {6},
  abstract = {Aim: \par
	
	- statistically based data preprocessing tool \par
	
	- should be able to: \par
	
	1. calculate appropriate level of spatial aggregation \par
	
	2. identify configuration of modeling units \par
	
	3. consider subwatersheds and HRUs in delineation \par
	
	
	Background: \par
	
	- homogeneous areas of watersheds result in input data redundancy
	\par
	
	- decreasing redundancy saves computation time and disk space \par
	
	- but: high degree of aggregation might affect model performance \par
	
	- goal: balance between maximum aggregation and minimum of information
	loss \par
	
	- SWAT: HRU concept with unique soil and land use information \par
	
	- topography included within subwatershed discretization \par
	
	
	Theory: \par
	
	- model efficiency increases with number of subwatersheds and approaches
	a maximum at a certain discretization level \par
	
	- when pattern of spatial modelling units represents combination of
	input parameter categories the maximum efficiency is reached \par
	
	- heterogeneity of spatial data can be quantified by entropy function
	\par
	
	- entropy is maximum where heterogeneity of spatial input data is
	high and where maximum of model efficiency is expected \par
	
	- advantage: several spatial input data can be studied individually
	or in combination by a single measure \par
	
	- assumption: if entropy of modeling units equals entropy of input
	parameters, model efficiency will be maximized \par
	
	- workflow: \par
	
	1. input map layers are combined / intersected to create a new map
	\par
	
	2. maximum entropy of new map is calculated \par
	
	3. define maximum of entropy by allowing for a percantage reduction
	in entropy from value calculated in 2. \par
	
	4. entropy function can be computed for grids by moving window technique:
	\par
	
	 - choose window of certain number of grids \par
	
	 - move window focusig on each grid of the new map \par
	
	 - calculate entropy of each window from categories of the new map
	that are covered \par
	
	 - sum entropy of all windows to get average entropy of the entire
	map \par
	
	 - repeat procedure with increased window size until average entropy
	reaches user defined maximum (3.) \par
	
	
	Tool SUSAT: \par
	
	- advantages: \par
	
	 1. objective choice of spatial heterogeneity instead of time consuming
	trial and error search \par
	
	 2. method is flexible, independent of the model, applicable to small
	and large watersheds, not limited to a number of input layers \par
	
	- limitations: \par
	
	 - only applicable on nominal data \par
	
	 - does not consider the influence of streamflow routing to the watershed
	outlet \par
	
	 - the optimum of spatial disaggregation is a subjective value (user-defined
	limit of acceptable information loss) \par},
  doi = {10.1111/j.1752-1688.2002.tb04377.x},
  file = {:Haverkamp_etal_2002_model_discretization_by_statistical_criteria.pdf:PDF},
  issn = {1752-1688},
  keywords = {watershed modeling, geographic information system, data preprocessing,
	spatial discretization, heterogeneity, entropy},
  publisher = {Blackwell Publishing Ltd}
}

