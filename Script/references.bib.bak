@ARTICLE{Weiler2015,
  author = {Weiler, Markus and Beven, Keith},
  title = {Do we need a Community Hydrological Model?},
  journal = {Water Resour. Res.},
  year = {2015},
  volume = {51},
  pages = {7777--7784},
  abstract = {- too many models in hydrology \par
	
	 - reasons: PhD students think they can do better, wide range of different
	types of applications with different needs, no agreement on a common
	set of concepts \par
	
	 - waste of community efforts and resources: the same of very similar
	concepts have been programmed and tested over and over again \par
	
	 - better initiatives in atmospheric modelling regarding community
	efforts but hydrological representation therein more focus on energy
	balance rather than discharge prediction \par
	
	 - trend in hydrology: create more new models or frameworks to combine
	different process representations \par
	
	 - approaches: conceptual vs. physically based \par
	
	 - no real convergence to a common goal \par
	
	
	- what a community hydrological model should look like \par
	
	 - many local to national attemps to define a community model \par
	
	 - general agreement that model strcuture organized by hillslopes
	and catchments is superior to raster grid based approaches \par
	
	 - mass, energy, and momentum balance equations can be applied at
	any discretization scale \par
	
	 - problem is to define scale-dependent closure schemes for boundary
	fluxes for discretization \par
	
	 - the way that hydrological processes are represented at different
	scales has to change \par
	
	
	- issues from discussion workshop: \par
	
	 - common standard and protocols for data needed to set up and run
	models \par
	
	 - detailed metadata \par
	
	 - benchmark data sets for different hydrological regimes \par
	
	 - problem: data standard and availability varies \par
	
	 - provide good numerical solution algorithms \par
	
	 - provide output metadata to defined standards for use in other models
	\par
	
	
	- agreement on modelling concepts \par
	
	 - different hydrologists will have different ideas about what is
	important \par
	
	 - different data availability \par
	
	 - different environmental conditions requiring different model functionality
	\par
	
	 - problems with continuum differential equation approach: field work
	shows that unsaturated zone hydrology does not follow gradient-based
	approach at any scales \par
	
	 - more community effort is needed to properly define subelement parameterizations
	at different spatial scales and in different hydrological regimes
	\par
	
	 - community modelling framework might be easier than a community
	model \par
	
	 - a guide for good practice (i.e. which model components to use when)
	and how to assess uncertainty would be needed \parm},
  doi = {10.1002/2014WR016731},
  file = {:Weiler_Beven_2015_need_of_community_hydrologic_model.pdf:PDF},
  issn = {1944-7973},
  keywords = {hydrological model, community model, perceptual models, model framework},
  owner = {tobias},
  timestamp = {2016.06.17}
}

