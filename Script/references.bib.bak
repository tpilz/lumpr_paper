@ARTICLE{Bogaart2006,
  author = {Bogaart, P. W. and Troch, P. A.},
  title = {Curvature distribution within hillslopes and catchments and its effect
	on the hydrological response},
  journal = {Hydrol. Earth Syst. Sc.},
  year = {2006},
  volume = {10},
  pages = {925--936},
  number = {6},
  abstract = {Aim:
	
	- discussion of controls on morphometric hillslope parameters, focussing
	on flow line convergence and divergence
	
	
	Background and definitions:
	
	- hydrological cycle driven by hydrometeorological fluxes and controlled
	by landscape properties
	
	- limitation of study: typical catchments of humid climates dominated
	by subsurface flow, saturation-excess overland flow, and open-channel
	flow
	
	 - well-defined perennial streams
	
	 - no pits (every point on hillslope drains into a channel)
	
	 - highly permeable soil layer on top of impermeable bedrock
	
	- categories of landscape properties controlling hydrological response:
	
	 - ecological factors
	
	 - hydraulic factors
	
	 - geometric factors
	
	 - not independent, feedbacks between hillslope hydrology and controlling
	factors exist
	
	- classical form of geomorphic information: DEM
	
	 - classical form to use it: 2-D or 3-D gridded spatially distributed
	model (explicit way)
	
	 - imlicit use of applying geomorphic data: respect larger scale geomorphic
	properties, e.g. using similarity indices for hydrological applications
	
	- catchment subdivision into fixed drainage network and fixed hillslope
	
	 - hillslope further broken down into individual homogeneous hillslopes
	characterized by morphometric parameters
	
	 - first order hillslope parameters: easily and directly observable
	from DEM (e.g. length, slope)
	
	 - second order hillslope parameters: more problematic (e.g. convergence,
	divergence)
	
	
	Morphometric parameters:
	
	- hillslope gradient
	
	 - controlled by tectonic and geomorphological setting
	
	- hillslope length
	
	 - related to drainage density
	
	 - defined by location of channel heads
	
	 - clear hydrologic significance
	
	 - controlled by sediment transport which is itself dependent on hillslope
	hydrology
	
	- profile curvature:
	
	 - steepest descent path
	
	 - derived from first and second order partical derivatives of topographic
	surface
	
	 - positive/negative values indicate concave/convex profiles
	
	- contour curvature (or planform curvature):
	
	 - measures curvature of contour lines
	
	 - related to convergence and divergence of flow lines
	
	
	Curvature, dominant processes, hydrological resonse
	
	- flowpath length is important parameters for hillslope models
	
	 - different algorithms: D8 (single flow) vs. multidirectional flow
	algorithms
	
	 - multi flow direction algorithm can take divergence into account
	in contrast to single direction algorithms
	
	 - flowpath length depends on algorithm
	
	- flow distance fields:
	
	 - width function as important parameters equivalent to frequency
	of flow distances
	
	 - also depends on flow direction algorithm
	
	- curvature and dominating processes:
	
	 - curvature can be analysed from width function and statistics of
	grid cells types
	
	 - majority of grid cells divergent but catchment-scale curvature
	convergent because small fraction of grid cells is strongly convergent
	
	 - small contributing area, contour curvature dominantly divergent,
	profile curvature convex, diffusional sediment transports dominate,
	dom-like shape
	
	 - transition zone, episodic transport in form of debris flow and/or
	shallow landsliding, hillslope hollows
	
	 - large contributing area, contour curvature dominantly convergent,
	semi-permanent channel network, fluvial sediment transport most dominant,
	valleys in between hillslopes
	
	- hydrological response:
	
	 - affected by within-hillslope curvature variability
	
	 - contrasting effects from divergent and convergent areas almost
	cancel out at total hydrograph
	
	 - justification for clustering of individual divergent and convergent
	hillslopes into a single hillslope width function},
  doi = {10.5194/hess-10-925-2006},
  file = {:Bogaart_Troch_2006_curvature_distribution_effects.pdf:PDF}
}

@INCOLLECTION{Branger2012,
  author = {Branger, Flora and Jankowfsky, Sonja and Vannier, Olivier and Viallet,
	Pierre and Debionne, Samuel and Braud, Isabelle},
  title = {Use of Open-Source GIS for the Pre-processing of Distributed Hydrological
	Models},
  booktitle = {Geospatial Free and Open Source Software in the 21st Century: Proceedings
	of the first Open Source Geospatial Research Symposium, OGRS 2009},
  publisher = {Springer Berlin Heidelberg},
  year = {2012},
  editor = {Bocher, Erwan and Neteler, Markus},
  chapter = {3},
  pages = {35--48},
  abstract = {Aim:
	
	- presenting and discussing experiences with free and open-source
	GIS for pre-processing operations in distributed hydrological modelling
	
	
	Motivation:
	
	- hydrological simulation models:
	
	 a) grid-based models: use pixels of a raster as elementary spatial
	units
	
	 b) vector-based models: use irregular vector geometries (e.g. HRUs
	or hydro-landscapes)
	
	- delineation of spatial units: depends on, e.g., objectives of the
	study, scale, model specifications, and data availability
	
	- during delineation: input data must be transformed, overlaid, etc.
	using GIS
	
	
	Application cases:
	
	- case studies: flash flood simulation in semi-mountainuous catchments,
	influence of landscape and water management practices, understanding
	and modelling capacity of water fluxes in suburban watershed
	
	- data each from various sources with variable quality: DEMs, maps
	of river network, data for geology, pedology, land use
	
	- software: verious free and open-source GIS under Windows tested
	and evaluated for each processing step
	
	
	Pre-processing steps:
	
	1. Delineation of subcatchments from DEM
	
	 - sink removal
	
	 - flow directions
	
	 - slopes
	
	 - drained areas
	
	 - river network delineation based on thresholds
	
	 - subcatchment delineation based on threshols
	
	2. Correction of topology
	
	 - detection and correction of inconsistencies in input data (overlaps,
	holes, spaces between polygons, etc.)
	
	3. Advanced editing of layers
	
	 - optional, case study and model specific steps
	
	 - merging: transformation of input (e.g. line features) into polygons
	and merging of several polygon data layers to derive one layer without
	overlaps
	
	 - intersection of lines and polygons: splitting polygons according
	to lines (e.g. land use polygons according to river network)
	
	 - identification of interfaces: common boundaries of different elements
	(important zones for lateral water exchanges in hydrological modelling)
	
	
	Conclusions:
	
	1. fully topological software (e.g. GRASS) is more convenient for
	processing vector data
	
	2. sometimes scripting facilities are necessary (some processing operations,
	automatisation, or for processing of large datasets)
	
	3. GRASS appeared to be most promising open-source tool
	
	4. detailed and most accurate pre-processing can only be done manually
	
	5. open-source GIS generally well suited for research purposes and
	quick software intercomparisons
	
	
	Limitations:
	
	- open source GIS often not easy to use as: not as user-friendly and
	well documented as proprietary software, maintenance depends on free
	time of developers, etc.
	
	- not well suited for non-experts},
  doi = {10.1007/978-3-642-10595-1_3},
  file = {:Branger_etal_2012_open-source_GIS_model_pre-processing.pdf:PDF},
  isbn = {978-3-642-10595-1}
}

